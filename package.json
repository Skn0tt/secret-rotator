{
  "name": "rotator",
  "version": "0.1.0",
  "main": "src/index.ts",
  "bin": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build:macOS": "yarn compile && yarn package:macOS",
    "build:alpine": "yarn compile && yarn package:alpine",
    "build:linux": "yarn compile && yarn package:linux",
    "compile": "yarn tsc",
    "package:macOS": "yarn pkg -t node9-macos-x64 .",
    "package:alpine": "yarn pkg -t node9-alpine-x64 .",
    "package:linux": "yarn pkg -t node9-linux-x64 ."
  },
  "dependencies": {
    "express": "^4.16.3",
    "express-wrap-async": "^1.0.3",
    "morgan": "^1.9.0",
    "redis": "^2.8.0"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.1.3",
    "@types/redis": "^2.8.6",
    "pkg": "^4.3.1",
    "typescript": "^2.8.3"
  }
}
